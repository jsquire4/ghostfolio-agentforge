[
  {
    "id": "gs-market-data-001",
    "description": "trigger phrase — current price single ticker",
    "input": { "message": "What's the current price of NVDA?" },
    "expect": {
      "toolsCalled": ["market_data"],
      "noToolErrors": true,
      "responseNonEmpty": true,
      "responseContains": ["NVDA"],
      "responseContainsAny": [["price", "trading", "worth", "valued"]],
      "responseNotContains": ["I don't know", "no information", "unable to"],
      "maxLatencyMs": 30000
    }
  },
  {
    "id": "gs-market-data-002",
    "description": "trigger phrase — performance with time range",
    "input": { "message": "How has META performed over the past year?" },
    "expect": {
      "toolsCalled": ["market_data"],
      "noToolErrors": true,
      "responseNonEmpty": true,
      "responseContains": ["META"],
      "responseContainsAny": [
        ["performance", "return", "gain", "loss", "change"]
      ],
      "responseNotContains": ["I don't know", "no information"],
      "maxLatencyMs": 30000
    }
  },
  {
    "id": "gs-market-data-003",
